        <a class="btn" href="#home"><i class="fa-solid fa-hashtag fa-fw"></i> Home</a>
        <a class="btn" href="#credits"><i class="fa-solid fa-hashtag fa-fw"></i> Credits</a>

        <a class="btn br" target="_blank" href="/legal/license" data-r="devanka761"><i class="fa-duotone fa-regular fa-up-right-from-square fa-fw"></i> License</a>
        <a class="btn" target="_blank" href="/legal/terms" data-r="devanka761"><i class="fa-duotone fa-regular fa-up-right-from-square fa-fw"></i> Terms of Service</a>
